
n = int(input())
 
for i in range(n):
    a =list(map(int,input().split()))
    if a[0] > a[1] and (a[0] - a[1]) % 2 == 0:
        print(1)
    elif a[0] > a[1] and (a[0] - a[1]) % 2 != 0:
        print(2)
    elif a[0] < a[1] and (a[1] - a[0]) % 2 == 0:
        print(2)
    elif a[0] < a[1] and (a[1] - a[0]) % 2 != 0:
        print(1)
    else:
        print(0)