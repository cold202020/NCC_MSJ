#include<iostream>
#include<iomanip>
using namespace std;
float a;
int main(){
cin>>a;
if (a<10)
cout<<setprecision(0)<<a;
else if (a>=10&&a<100)
cout<<setprecision(2)<<a;
else if(a>=100&&a<1000)
cout<<setprecision(3)<<a;
else if(a>=1000&&a<10000)
cout<<setprecision(4)<<a;
else if(a>=10000&&a<100000)
cout<<setprecision(5)<<a;
}